<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ahv1.app.autohelpv2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".Cadastro_Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.RespostaActivity"
            android:parentActivityName=".MainActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahv1.app.autohelpv2.MainActivity" />
        </activity>

        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Cadastro_Login.SignUp"></activity>

        <uses-sdk
            android:minSdkVersion="11"
            android:targetSdkVersion="17" />

    </application>

</manifest>